{
    "name": "rkw/rkw-solrsearch",
    "type": "typo3-cms-extension",
    "description": "Extension for website search. Primarily for TYPO3 SOLR.",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Solr"],
    "support": {
      "email": "c.dilger@addorange.de"
    },
    "authors": [
        {
          "name": "Maximilian Fäßler",
          "email": "maximilian@faesslerweb.de",
          "homepage": "https://www.faesslerweb.de",
          "role": "Developer"
        },
        {
            "name": "Christian Dilger",
            "email": "c.dilger@addorange.de",
            "role": "Developer"
        }
    ],
    "require": {
      "php": ">=7.4",
      "typo3/cms-core": "~10.4.0",
      "rkw/rkw-events": "~10.4.0",
      "apache-solr-for-typo3/solr": "~11.2.0"
    },
    "autoload": {
        "psr-4": {
            "RKW\\RkwSolrsearch\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RKW\\RkwSolrsearch\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_solrsearch"
        }
    }
}

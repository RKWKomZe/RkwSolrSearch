{
    "name": "fsl/searchmaster",
    "type": "typo3-cms-extension",
    "description": "Extension for website search. Primarily for TYPO3 SOLR.",
    "authors": [
        {
            "name": "Maximilian F\u00e4\u00dfler",
            "role": "Developer"
        },
		{
			"name": "Christian Dilger",
			"email": "c.dilger@addorange.de",
			"role": "Developer"
		}
    ],
    "require": {
      "php": ">=7.4",
      "typo3/cms-core": "~9.5.0",
      "rkw/rkw-events": "~9.5.0",
      "apache-solr-for-typo3/solr": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "FSL\\Searchmaster\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSL\\Searchmaster\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/searchmaster": "self.version"
    }
}
